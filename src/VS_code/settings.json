{
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.wordWrapColumn": 80,
    "editor.minimap.enabled": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["J"],
            "after": ["1", "0", "j"],
        },
        {
            "before": ["K"],
            "after": ["1", "0", "k"],
        },
    ],
    "vim.textwidth": 80,
    "explorer.confirmDelete": false
}
